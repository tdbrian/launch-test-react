{
  "name": "launch-test",
  "version": "1.0.0",
  "description": "Manage and test web application UI's through an on premise GUI.",
  "main": "index.js",
  "scripts": {
    "start": "npm-run-all --parallel test:watch webpack:dev",
    "webpack:dev": "webpack-dev-server --hot --inline --colors --open",
    "test": "mocha --reporter spec --compilers coffee:coffee-script/register tools/testSetup.coffee \"src/**/*.test.coffee\"",
    "test:watch": "npm run test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tdbrian/launch-test.git"
  },
  "keywords": [
    "gui",
    "ui",
    "test",
    "framework",
    "testing",
    "tester",
    "web",
    "application",
    "manager",
    "manage"
  ],
  "author": "Thomas Brian <github.com/tdbrian>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/tdbrian/launch-test/issues"
  },
  "homepage": "https://github.com/tdbrian/launch-test#readme",
  "dependencies": {},
  "devDependencies": {
    "bootstrap": "3.3.6",
    "coffee-loader": "^0.7.2",
    "coffee-react-transform": "^3.1.0",
    "coffee-script": "^1.11.0",
    "colors": "1.1.2",
    "compression": "1.6.1",
    "cross-env": "1.0.7",
    "css-loader": "0.23.1",
    "enzyme": "2.2.0",
    "expect": "1.19.0",
    "express": "4.13.4",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.8.5",
    "immutable": "^3.8.1",
    "jquery": "2.2.3",
    "jsdom": "8.5.0",
    "jsx-loader": "^0.11.0",
    "mocha": "2.4.5",
    "nock": "8.0.0",
    "npm-run-all": "1.8.0",
    "open": "0.0.5",
    "react": "^0.14.7",
    "react-addons-test-utils": "^15.3.2",
    "react-dom": "^0.14.7",
    "react-hot-loader": "^1.3.0",
    "react-redux": "4.4.5",
    "react-router": "2.4.0",
